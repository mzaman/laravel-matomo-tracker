{
    "name": "mzaman/laravel-matomo-tracker",
    "version": "2.0.0",
    "description": "A Laravel facade/wrapper for the matomo/matomo-php-tracker for server-side Matomo tracking.",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Masud Zaman",
            "email": "masud.zmn@gmail.com",
            "homepage": "https://masudzaman.com"
        }
    ],
    "homepage": "https://github.com/mzaman/laravel-matomo-tracker",
    "keywords": ["Laravel", "Matomo Tracker", "Piwik", "Piwik Laravel Tracker", "Server side tracking"],
    "require": {
        "php": "^8.1",
        "matomo/matomo-php-tracker": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0",
        "sempro/phpunit-pretty-print": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "MasudZaman\\MatomoTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MasudZaman\\MatomoTracker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider='MasudZaman\\MatomoTracker\\LaravelMatomoTrackerServiceProvider'"
        ],
        "test": [
            "phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MasudZaman\\MatomoTracker\\LaravelMatomoTrackerServiceProvider"
            ],
            "aliases": {
                "LaravelMatomoTracker": "MasudZaman\\MatomoTracker\\Facades\\LaravelMatomoTracker"
            }
        }
    }
}
