{
    "name": "mzaman/laravel-matomo-tracker",
    "description": "A Laravel facade/wrapper for the matomo/matomo-php-tracker for server side Matomo tracking.",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Masud Zaman",
            "email": "masud.zmn@gmail.com",
            "homepage": "https://masudzaman.com"
        }
    ],
    "homepage": "https://github.com/mzaman/matomotracker",
    "keywords": ["Laravel", "Matomo PHP Tracker", "Piwik", "Piwik PHP Tracker", "server side tracking"],
    "require": {
        "matomo/matomo-php-tracker": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3 || ^10.1",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MasudZaman\\MatomoTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MasudZaman\\MatomoTracker\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MasudZaman\\MatomoTracker\\LaravelMatomoTrackerServiceProvider"
            ],
            "aliases": {
                "LaravelMatomoTracker": "MasudZaman\\MatomoTracker\\Facades\\LaravelMatomoTracker"
            }
        }
    }
}
